%%EndComments
%%BeginProlog

(/home/pjb/ps/lib/colours.ps) run
/xmax currentpagedevice (PageSize) get 0 get def
/ymax currentpagedevice (PageSize) get 1 get def

%%EndProlog
%%Page: 1 1
%%BeginPageSetup
%%EndPageSetup
 50 100 500 170 darkblue white    true -0.8 rectgradientfill
 50 270 500 500 white    paleblue true  0.3 rectgradientfill
showpage

%%Page: 2 2
%%BeginPageSetup
%%EndPageSetup
/gray 0.5 def
red    gray adjustbrightness
   setrgbcolor  0 0           xmax ymax .2 mul rectfill
orange gray adjustbrightness
   setrgbcolor  0 ymax .2 mul xmax ymax .2 mul rectfill
yellow gray adjustbrightness
   setrgbcolor  0 ymax .4 mul xmax ymax .2 mul rectfill
green  gray adjustbrightness
   setrgbcolor  0 ymax .6 mul xmax ymax .2 mul rectfill
blue   gray adjustbrightness
   setrgbcolor  0 ymax .8 mul xmax ymax .2 mul rectfill
showpage

%%Page: 3 3
%%BeginPageSetup
%%EndPageSetup
/Helvetica-Bold 20 selectfont
/arr_in [ darkgrey  black red orange yellow blue violet green ] def
/y ymax 10 sub def
/dx xmax 20 sub 5 div def
{
  /y y 25 sub def
  /arr_out  arr_in 5 randomrgbgetn def
  /x 10 def
  20 y moveto
  0  1  arr_out rgblength 1 sub {
    arr_out exch rgbget setrgbcolor  x y dx 25 rectfill
    /x x dx add def
  } for
  y 10 lt { exit } if
} loop
showpage


