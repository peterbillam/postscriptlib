%
% redisplay with . lots of times for best effect
%%EndComments
%%BeginProlog
(/home/pjb/ps/lib/colours.ps) run
(/home/pjb/ps/lib/random.ps) run
/xmax currentpagedevice (PageSize) get 0 get def
/ymax currentpagedevice (PageSize) get 1 get def
/xmid xmax 0.5 mul def    /ymid ymax 0.5 mul def
/paint {
  newpath
  /gauss_rand_already false def
  /xm xmid 0.51 mul def
  /ym ymid 0.51 mul def
  xm irand ym irand xmid 0.17 mul xmid 0.06 mul grand 0 360 arc
    darkorange setrgbcolor fill
  xm irand ym irand xmid 0.16 mul xmid 0.05 mul grand 0 360 arc
    orange setrgbcolor fill
  xm irand ym irand xmid 0.15 mul xmid 0.04 mul grand 0 360 arc
    red    setrgbcolor fill
  xm irand ym irand xmid 0.14 mul xmid 0.04 mul grand 0 360 arc
    violet setrgbcolor fill
  xm irand ym irand xmid 0.12 mul xmid 0.037 mul grand 0 360 arc
    yellow setrgbcolor fill
} def
%%EndProlog

%%Page: 1 1
%%BeginPageSetup
%%EndPageSetup
/horizon ymax .22 mul def
0 0       xmax horizon          darkblue white    true -0.8 rectgradientfill
0 horizon xmax ymax horizon sub white    paleblue true  0.3 rectgradientfill

xmid ymax 0.55 mul translate

rand rand
/starting_rand 100000 irand def
/tan30 30 sin 30 cos div def
%/starting_rand 1001 def
gsave newpath
  0 0 moveto 0 ymid lineto  xmid ymid lineto  xmid xmid tan30 mul lineto
  closepath
  clip starting_rand srand paint
grestore
gsave newpath
  120 rotate
  0 0 moveto 0 ymid lineto  xmid ymid lineto  xmid xmid tan30 mul lineto
  closepath
  clip starting_rand srand paint
grestore
gsave newpath
  240 rotate
  0 0 moveto 0 ymid lineto  xmid ymid lineto  xmid xmid tan30 mul lineto
  closepath
  clip starting_rand srand paint
grestore
gsave
  -1 1 scale newpath
  0 0 moveto 0 ymid lineto  xmid ymid lineto  xmid xmid tan30 mul lineto
  closepath
  clip starting_rand srand paint
grestore
gsave
  120 rotate -1 1 scale newpath
  0 0 moveto 0 ymid lineto  xmid ymid lineto  xmid xmid tan30 mul lineto
  closepath
  clip starting_rand srand paint
grestore
gsave
  240 rotate -1 1 scale newpath
  0 0 moveto 0 ymid lineto  xmid ymid lineto  xmid xmid tan30 mul lineto
  closepath
  clip starting_rand srand paint
grestore

showpage

